package q6;

/**
 *
 * @author chirag
 */
class Student{
    String name;
   Student(){
       this.name = "unknown";
   }
   Student(String name){
       this.name = name;
   }
   void print(){
       System.out.println(this.name);
   }
}
public class Q6 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Student s1 = new Student();
        Student s2 = new Student("chirag");
        s1.print();
        s2.print();
    }
    
}

